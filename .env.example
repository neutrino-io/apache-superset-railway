# Superset Admin Configuration
ADMIN_USERNAME=admin
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=CHANGE_ME_GENERATE_STRONG_PASSWORD

# Security Keys - Generate with: python3 -c "import secrets; print(secrets.token_urlsafe(32))"
SECRET_KEY=CHANGE_ME_GENERATE_NEW_KEY
SUPERSET_SECRET_KEY=CHANGE_ME_GENERATE_NEW_KEY

# PostgreSQL Database URI
# Format: postgresql://user:password@host:port/database
SQLALCHEMY_DATABASE_URI=postgresql://user:password@host:port/database

# Application Settings
PORT=8088
SUPERSET_ENV=production
FLASK_APP=superset.app:create_app()

# IMPORTANT: Never commit real credentials to git!
# Set these via Railway dashboard environment variables
